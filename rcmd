#!/bin/bash
IP="192.168.0.$1"
RCMD="$2"
[ "$2" == "gitAll" ] && export RCMD="/home/pi/APC_TestTools/gitAll"
[ "$2" == "getvpn" ] && export RCMD="/home/pi/APC_TestTools/getvpn"
[ "$2" == "stopRPi.pl" ] && export RCMD="/home/pi/APC_TestTools/getvpn"
[ "$2" == "grabAll" ] && export RCMD="/home/pi/APC_TestTools/getvpn"
echo "$RCMD at $IP"
ssh pi@"$IP" "$RCMD"
