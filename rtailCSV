#!/bin/bash
IP="192.168.0.$1"
RPATH="$2"
[ "$2" == "MPU" ] && export RPATH="/home/pi/I2C/i2cdata/$2"
[ "$2" == "door" ] && export RPATH="/home/pi/I2C/i2cdata/$2"
[ "$2" == "dB" ] && export RPATH="/home/pi/I2C/i2cdata/$2"
[ "$2" == "voltage" ] && export RPATH="/home/pi/I2C/i2cdata/$2"
[ "$2" == "temp" ] && export RPATH="/home/pi/I2C/i2cdata/$2"
[ "$2" == "doortemp" ] && export RPATH="/home/pi/I2C/i2cdata/$2"
[ "$2" == "gpio" ] && export RPATH="/home/pi/I2C/i2cdata/$2"
[ "$2" == "I2C" ] && export RPATH="/home/pi/I2C/i2cdata"
[ "$2" == "png" ] && export RPATH="/data/NVR"
echo "$RPATH at $IP"
ssh pi@"$IP" "/home/pi/APC_TestTools/tailCSV $RPATH"
